<br>
<div class="booyah-box col-6 offset-4">
  <div class="card">
    <div class="card-body">
      <h1>My Games:</h1>
      <% @user_games.each do |game| %>
        <p><%= link_to game.name, game_path(game) %></p>
      <% end %>
    </div>
  </div>
  <div class="card">
    <div class="card-body">
      <h1>My Wins:</h1>
      <%= current_user.wins %>
    </div>
  </div>
</div>
